<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>
      netstandard2.1;
      netstandard2.0;
      net45;
      net461;
    </TargetFrameworks>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <Version>1.0.0</Version>
    <Authors>windnight</Authors>
    <Company></Company>
    <Description>Phonedata_NetCore</Description>
    <PackageReleaseNotes>Phonedata_NetCore</PackageReleaseNotes>
    <Title>$(AssemblyName)</Title>
    <PackageIconUrl>//</PackageIconUrl>
    <PackageProjectUrl>https://github.com/windnight/Phonedata_NetCore</PackageProjectUrl>
    <PackageLicenseUrl>//</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Phonedata_NetCore.xml</DocumentationFile>
  </PropertyGroup>
 
  <ItemGroup>
    <EmbeddedResource Include="phone.dat">
      <LogicalName>phone.dat</LogicalName>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

</Project>
